// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
    relationMode = "prisma"
}

model Player {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  role      Position 
  adp       Float 
  team      String  @default("")
  bye       Int     @default(-1)
}


enum Position {
  QB
  WR
  TE
  RB
  DST
  K
}